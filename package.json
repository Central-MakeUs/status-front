  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/navermaps": "^3.9.1",
    "@types/node": "^24.0.3",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.2",
    "autoprefixer": "^10.4.21",
